pipeline {
  agent any
    stages {
    stage ('SCM checkout') {
      steps {
        sh 'echo "git checkout "'
      }
    }
    stage ('Build') {
      steps {
        sh 'echo "mvn clean install"'
      }
    }
    stage ('Deploy') {
      steps {
        sh 'echo "rsync -arzvh ${WORKSPACE}/target/*.war' /opt/tomcat/apache-tomcat-9.0.68/webapps/"'
      }
    }
  }
}
